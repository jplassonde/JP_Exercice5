include "m8c.inc"


;--------------------------------------------------
; Constants for Timer24 API's.
;--------------------------------------------------

Timer24_CONTROL_REG_START_BIT:             equ 0x01    ; Control register start bit
Timer24_INT_REG:                           equ 0x0e1
Timer24_INT_MASK:                          equ 0x04


;--------------------------------------------------
; Constants for Timer24 user defined values
;--------------------------------------------------

Timer24_PERIOD:                            equ 0x0
Timer24_COMPARE_VALUE:                     equ 0x0


;--------------------------------------------------
; Register Address Constants for Timer24
;--------------------------------------------------

Timer24_COUNTER_LSB_REG:               equ 0x20   ; DR0 Counter register
Timer24_COUNTER_ISB_REG:               equ 0x24
Timer24_COUNTER_MSB_REG:               equ 0x28
Timer24_PERIOD_LSB_REG:                equ 0x21   ; DR1 Period register
Timer24_PERIOD_ISB_REG:                equ 0x25
Timer24_PERIOD_MSB_REG:                equ 0x29
Timer24_COMPARE_LSB_REG:               equ 0x22   ; DR2 CompareValue register
Timer24_COMPARE_ISB_REG:               equ 0x26
Timer24_COMPARE_MSB_REG:               equ 0x2a
Timer24_CONTROL_LSB_REG:               equ 0x23   ; Control register
Timer24_CONTROL_ISB_REG:               equ 0x27
Timer24_CONTROL_MSB_REG:               equ 0x2b
Timer24_FUNC_LSB_REG:                  equ 0x20   ; Function register
Timer24_FUNC_ISB_REG:                  equ 0x24
Timer24_FUNC_MSB_REG:                  equ 0x28
Timer24_INPUT_LSB_REG:                 equ 0x21   ; Input register
Timer24_INPUT_ISB_REG:                 equ 0x25
Timer24_INPUT_MSB_REG:                 equ 0x29
Timer24_OUTPUT_LSB_REG:                equ 0x22   ; Output register
Timer24_OUTPUT_ISB_REG:                equ 0x26
Timer24_OUTPUT_MSB_REG:                equ 0x2a


;--------------------------------------------------
; Timer24 Macro 'Functions'
;--------------------------------------------------

   macro Timer24_Start_M
   or    reg[Timer24_CONTROL_LSB_REG],  Timer24_CONTROL_REG_START_BIT
   endm

   macro Timer24_Stop_M
   and   reg[Timer24_CONTROL_LSB_REG], ~Timer24_CONTROL_REG_START_BIT
   endm

   macro Timer24_EnableInt_M
   M8C_EnableIntMask Timer24_INT_REG, Timer24_INT_MASK
   endm

   macro Timer24_DisableInt_M
   M8C_DisableIntMask Timer24_INT_REG, Timer24_INT_MASK
   endm


; end of file Timer24.inc
